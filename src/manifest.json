	"js": ["js/core.js", "js/imageview.js","js/analytics.js", "js/util.js","js/actions.js", "js/encodeToGb2312.js", "js/enginescontentscript.js", "js/savecontentscript.js","js/superdrag.js"],
    	"matches": ["http://*/*", "https://*/*"],
        "run_at": "document_start"
  }
],
"default_locale": "zh_CN",
"permissions": [
  "tabs",
  "http://*/*", 
  "https://*/*"
],
"options_page": "options.html",
