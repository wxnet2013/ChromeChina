{
"name":"QuickSearch",
"version":"1.9.6",
"description":"超级拖拽功能、将自定义的搜索引擎添加到右键菜单中.",
"background_page": "background.html",
   "icons": {
      "128": "images/search-icon-128x128.png"
   },
"content_scripts": [
  {
	"css": ["css/content.css"],
	"js": ["js/core.js", "js/imageview.js", "js/util.js","js/actions.js", "js/encodeToGb2312.js", "js/enginescontentscript.js", "js/savecontentscript.js","js/superdrag.js"],
    	"matches": ["http://*/*", "https://*/*"],
        "run_at": "document_start"
  }
],
"default_locale": "zh_CN",
"permissions": [
  "tabs",
  "http://*/*", 
  "https://*/*"
],
"options_page": "options.html",
"update_url": "http://searchforchrome.googlecode.com/svn/trunk/update.xml"
}